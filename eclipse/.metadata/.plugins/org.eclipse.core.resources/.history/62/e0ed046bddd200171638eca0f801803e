package codechef;
import java.util.Scanner;
public class SquareRoot{
	public static void main(String args[]){
	Scanner in = new Scanner(System.in);
	int n = in.nextInt();
	int y=1;
	int x=n;
 while(x > y) //5>1,2>1
  {
    x = (x + y)/2; //x=((5+1)/2)=3 ; x=((2+1)/2)=1 
    y = n/x; //6/3 =2 ;6/1=1
  }
  
	System.out.println(y);
	}
}
