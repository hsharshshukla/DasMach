class Node{
	int key;
	Node left,right;
	public Node(int item){
	key = item;
	left = right = null;
	}
}

class BinTree{
	Node root;
	
	BinTree(int key){
		root = new Node(key);
	}
	BinTree()
	{
		root = null;
	}
	public static void main(String args[]){
		BinTree tree = new BinTree();
//Create root 
		tree.root = new Node(1);
//Creating Child to tree
		tree.root.left = new Node(2);
		tree.root.right = new Node (3);
	}
}

