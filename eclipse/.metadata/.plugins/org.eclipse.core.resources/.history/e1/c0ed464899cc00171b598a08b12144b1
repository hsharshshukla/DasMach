class Node {
	int key;
	Node left , right;
	public Node (int item){
		key = item;
		right  =left = null;
	}
}
//Binary Tree
class BinTree{
	Node root;
	BinTree(int key)
	{
		root = new Node(key);
		
	}
	BinTree(){
		root = null;
	}
	
	public static void main(String args[]){
		BinTree tree = new BinTree();
		tree.root = new Node(1);
		tree.root.left = new Node(2);
		tree.root.right = new Node(3);
	}
}